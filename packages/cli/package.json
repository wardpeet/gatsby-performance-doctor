{
  "name": "gatsby-performance-doctor",
  "version": "0.0.1",
  "description": "A utility plugin that help diagnose performance on gatsby sites",
  "main": "index.js",
  "author": "Ward Peeters <ward@coding-tech.com>",
  "license": "MIT",
  "private": "true",
  "bin": {
    "gatsby-performance-doctor": "./cli/bin.js"
  },
  "scripts": {
    "start": "node ./cli/bin.js",
    "dev": "ts-node ./src/cli/bin.ts",
    "build": "yarn clean && tsc -p .",
    "clean": "del-cli \"cli/**/*\" gatsby-node.js gatsby-browser.js tsconfig.tsbuildinfo",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@types/puppeteer": "^2.0.1",
    "@types/sade": "^1.6.0",
    "del-cli": "^3.0.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "cli-table3": "^0.5.1",
    "fs-extra": "^8.1.0",
    "got": "^10.6.0",
    "puppeteer": "^2.1.1",
    "sade": "^1.7.3"
  },
  "prettier": {
    "singleQuote": true
  },
  "files": [
    "cli/**",
    "gatsby-node.js",
    "gatsby-browser.js"
  ],
  "workspaces": [
    "packages/gatsby-plugin-route-dictionary"
  ]
}